#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ES28845103K/Documents/Proyectos/Crussader/web/web/node_modules/.pnpm/tsup@8.5.0_jiti@2.5.1_postc_331cec6ffda8fd968d55e973c26a537f/node_modules/tsup/dist/node_modules:/mnt/c/Users/ES28845103K/Documents/Proyectos/Crussader/web/web/node_modules/.pnpm/tsup@8.5.0_jiti@2.5.1_postc_331cec6ffda8fd968d55e973c26a537f/node_modules/tsup/node_modules:/mnt/c/Users/ES28845103K/Documents/Proyectos/Crussader/web/web/node_modules/.pnpm/tsup@8.5.0_jiti@2.5.1_postc_331cec6ffda8fd968d55e973c26a537f/node_modules:/mnt/c/Users/ES28845103K/Documents/Proyectos/Crussader/web/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ES28845103K/Documents/Proyectos/Crussader/web/web/node_modules/.pnpm/tsup@8.5.0_jiti@2.5.1_postc_331cec6ffda8fd968d55e973c26a537f/node_modules/tsup/dist/node_modules:/mnt/c/Users/ES28845103K/Documents/Proyectos/Crussader/web/web/node_modules/.pnpm/tsup@8.5.0_jiti@2.5.1_postc_331cec6ffda8fd968d55e973c26a537f/node_modules/tsup/node_modules:/mnt/c/Users/ES28845103K/Documents/Proyectos/Crussader/web/web/node_modules/.pnpm/tsup@8.5.0_jiti@2.5.1_postc_331cec6ffda8fd968d55e973c26a537f/node_modules:/mnt/c/Users/ES28845103K/Documents/Proyectos/Crussader/web/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-default.js" "$@"
fi
